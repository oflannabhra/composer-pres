{
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Seldaek/monolog"
        }
    ],
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "require": {
        "php": ">=5.3.2",
        "doctrine/orm": "@dev",
        "guzzlehttp/guzzle": "~4.0",
        "monolog/monolog": "1.12.0"
    },
    "scripts": {
        "post-package-install": "php -v",
        "post-install-cmd": "php -r 'echo \"Done Installing!\n\";'",
        "pre-install-cmd": "composer clearcache"
    }
}